<div class="my-5">
    <h1 class="text-center mb-5">사용자목록</h1>
    <div id="div_users"></div>
</div>

<script id="temp_users" type="x-handlebars-template">
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>이름</th>
                <th>전화번호</th>
                <th>주소</th>
            </tr>
        </thead>
        <tbody>
        {{#each .}}
            <tr>
                <td>{{uid}}</td>
                <td>{{uname}}</td>
                <td>{{phone}}</td>
                <td>{{address1}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</script>

<script>
    $.ajax({
        type:"get",
        url:"/users/list.json",
        success:function(data) {
            console.log(data);
            const temp=Handlebars.compile($("#temp_users").html());
            $("#div_users").html(temp(data));
        }
    });
</script>